<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.vueadminjava2.dao.BookDao">

    <resultMap type="com.example.vueadminjava2.entity.Book" id="BookMap">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="price" column="price" jdbcType="NUMERIC"/>
        <result property="created" column="created" jdbcType="TIMESTAMP"/>
        <result property="updated" column="updated" jdbcType="TIMESTAMP"/>
        <result property="imageUrl" column="image_url" jdbcType="VARCHAR"/>
        <result property="specialStartDateTime" column="special_start_date_time" jdbcType="TIMESTAMP"/>
        <result property="specialEndDateTime" column="special_end_date_time" jdbcType="TIMESTAMP"/>
        <result property="stock" column="stock" jdbcType="DOUBLE"/>
        <result property="discountPrice" column="discount_price"/>
        <result property="creater" column="creater"/>
    </resultMap>


</mapper>

